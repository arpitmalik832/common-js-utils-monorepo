{
  "name": "common-js-utils-monorepo",
  "version": "0.0.0",
  "description": "A Common JS Utils Monorepo",
  "type": "module",
  "author": "Arpit Malik",
  "license": "MIT",
  "engines": {
    "node": ">=20.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitmalik832/common-js-utils-monorepo"
  },
  "scripts": {
    "preinstall": "if [ \"$SKIP_PREINSTALL\" != \"true\" ]; then node -e \"if(process.env.npm_execpath.indexOf('pnpm') === -1) throw new Error('You must use pnpm to install dependencies')\"; else echo 'Skipping preinstall script'; fi",
    "rm-node_modules": "rimraf '**/node_modules' --glob",
    "rm-pnpm-lock": "rimraf pnpm-lock.yaml",
    "clean": "nx reset && pnpm rm-pnpm-lock && pnpm rm-node_modules",
    "build:pkg": "nx run-many --target=build --projects=\"@arpitmalik832/*\"",
    "build:pkg:beta": "nx run-many --target=build:beta --projects=\"@arpitmalik832/*\"",
    "build:pkg:prod": "nx run-many --target=build:prod --projects=\"@arpitmalik832/*\"",
    "build:test-apps": "nx run-many --target=build --projects=\"*_test\"",
    "build:test-apps:beta": "nx run-many --target=build:beta --projects=\"*_test\"",
    "build:test-apps:prod": "nx run-many --target=build:prod --projects=\"*_test\"",
    "build": "pnpm build:pkg && pnpm build:test-apps",
    "build:beta": "pnpm build:pkg:beta && pnpm build:test-apps:beta",
    "build:prod": "pnpm build:pkg:prod && pnpm build:test-apps:prod",
    "flow:check": "nx run-many --target=flow:check --all",
    "flow:check-all": "nx run-many --target=flow:check-all --all",
    "flow:status": "nx run-many --target=flow:status --all",
    "lint-js:self": "eslint --quiet",
    "lint-js:self:debug": "eslint --debug",
    "lint-js:self:print-config": "eslint --print-config eslintConfigProcessed.js",
    "prettier:self": "prettier --check",
    "lint:self": "pnpm lint-js:self \"**/*.{mjs,cjs,js,jsx,mdx}\" && pnpm prettier:self .",
    "lint-js:self:fix": "pnpm lint-js:self --fix",
    "prettier:self:fix": "prettier --write --ignore-unknown",
    "lint:self:fix": "pnpm lint-js:self:fix \"**/*.{mjs,cjs,js,jsx,mdx}\" && pnpm prettier:self:fix .",
    "lint-tsc": "nx run-many --target=lint:tsc --all",
    "lint": "nx run-many --target=lint --all && pnpm lint:self",
    "lint:fix": "nx run-many --target=lint:fix --all && pnpm lint:self:fix",
    "lint-staged": "nx run-many --target=lint-staged --all --parallel=1",
    "grant-permissions:husky": "chmod +x .husky/* && chmod +x git-hooks",
    "prepare": "husky && pnpm grant-permissions:husky",
    "commit": "git-cz",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test:watch --all",
    "test:ss-update": "nx run-many --target=test:ss-update --all",
    "test:coverage": "nx run-many --target=test:coverage --all",
    "test:clearCache": "nx run-many --target=test:clearCache --all",
    "test:watch:quick": "nx run-many --target=test:watch:quick --all",
    "test:watch:inBand": "nx run-many --target=test:watch:inBand --all",
    "test:coverage:silent": "nx run-many --target=test:coverage:silent --all",
    "test:coverage:quick": "nx run-many --target=test:coverage:quick --all",
    "test:coverage:inBand": "nx run-many --target=test:coverage:inBand --all",
    "test:coverage:silent:quick": "nx run-many --target=test:coverage:silent:quick --all",
    "test:coverage:silent:inBand": "nx run-many --target=test:coverage:silent:inBand --all",
    "knip": "nx run-many --target=knip --all",
    "knip:debug": "nx run-many --target=knip:debug --all",
    "knip:check-files": "nx run-many --target=knip:check-files --all",
    "knip:w/o-devDependencies": "nx run-many --target=knip:w/o-devDependencies --all",
    "knip:prod": "nx run-many --target=knip:prod --all",
    "knip:prod:check-files": "nx run-many --target=knip:prod:check-files --all",
    "e2e-open": "nx run-many --target=e2e-open --parallel=1 --all",
    "e2e-run": "nx run-many --target=e2e-run --parallel=1 --all",
    "e2e-open-all": "nx run-many --target=e2e-open-all --parallel=1 --all",
    "e2e-run-all": "nx run-many --target=e2e-run-all --parallel=1 --all",
    "storybook:build:pkg": "nx run-many --target=storybook:build --projects=\"@arpitmalik832/*\"",
    "storybook:build:pkg:beta": "nx run-many --target=storybook:build:beta --projects=\"@arpitmalik832/*\"",
    "storybook:build:pkg:prod": "nx run-many --target=storybook:build:prod --projects=\"@arpitmalik832/*\"",
    "storybook:build:test-apps": "nx run-many --target=storybook:build --projects=\"*_test\"",
    "storybook:build:test-apps:beta": "nx run-many --target=storybook:build:beta --projects=\"*_test\"",
    "storybook:build:test-apps:prod": "nx run-many --target=storybook:build:prod --projects=\"*_test\"",
    "storybook:build": "pnpm storybook:build:pkg && pnpm storybook:build:test-apps",
    "storybook:build:beta": "pnpm storybook:build:pkg:beta && pnpm storybook:build:test-apps:beta",
    "storybook:build:prod": "pnpm storybook:build:pkg:prod && pnpm storybook:build:test-apps:prod",
    "version-bump:patch:release": "pnpx release-it patch --ci",
    "version-bump:minor:release": "pnpx release-it minor --ci",
    "version-bump:major:release": "pnpx release-it major --ci",
    "version-bump:patch:alpha": "pnpx release-it patch prerelease --preRelease=alpha --ci",
    "version-bump:minor:alpha": "pnpx release-it minor prerelease --preRelease=alpha --ci",
    "version-bump:major:alpha": "pnpx release-it major prerelease --preRelease=alpha --ci",
    "version-bump:patch:beta": "pnpx release-it patch prerelease --preRelease=beta --ci",
    "version-bump:minor:beta": "pnpx release-it minor prerelease --preRelease=beta --ci",
    "version-bump:major:beta": "pnpx release-it major prerelease --preRelease=beta --ci",
    "version-bump:patch:rc": "pnpx release-it patch prerelease --preRelease=rc --ci",
    "version-bump:minor:rc": "pnpx release-it minor prerelease --preRelease=rc --ci",
    "version-bump:major:rc": "pnpx release-it major prerelease --preRelease=rc --ci"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@nx/workspace": "^20.4.6",
    "@release-it/conventional-changelog": "^10.0.0",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nx": "^20.4.6",
    "prettier": "^3.5.2",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "validate-branch-name": "^1.3.2"
  },
  "packageManager": "pnpm@10.5.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bundled-es-modules/glob",
      "@parcel/watcher",
      "cypress",
      "esbuild",
      "nx",
      "style-dictionary"
    ]
  }
}
